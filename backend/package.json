    "cors": "^2.8.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-async-errors": "^3.1.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.0",
    "helmet": "^6.0.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.0",
    "nodemailer": "^7.0.3",
    "redis": "^4.6.0",
    "sharp": "^0.32.0",
    "swagger-jsdoc": "^6.2.0",
